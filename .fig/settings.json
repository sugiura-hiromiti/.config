{
	"app.beta": true,
	"app.hideMenubarIcon": true,
	"app.preferredTerminal": "iTerm2",
	"autocomplete.firstTokenCompletion": true,
	"autocomplete.fuzzySearch": true,
	"autocomplete.height": 400,
	"autocomplete.keybindings.control+k": "toggleDescription",
	"autocomplete.keybindings.control+n": "navigateDown",
	"autocomplete.keybindings.control+p": "navigateUp",
	"autocomplete.keybindings.down": "ignore",
	"autocomplete.keybindings.enter": "insertSelectedAndExecute",
	"autocomplete.keybindings.escape": "hideAutocomplete",
	"autocomplete.keybindings.shift+tab": "ignore",
	"autocomplete.keybindings.tab": "insertSelected",
	"autocomplete.keybindings.up": "ignore",
	"autocomplete.scrollWrapAround": true,
	"autocomplete.theme": "system",
	"autocomplete.width": 500,
	"cli.tips.disabled": true,
	"dotfiles.syncImmediately": false,
	"figterm.csi-u.enabled": true,
	"integrations.hyper.disabled": true,
	"integrations.vscode.disabled": true
}
