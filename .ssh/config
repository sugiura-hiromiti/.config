# Host github.com
# 	AddKey

Host github.personal
	AddKeysToAgent yes
	UseKeychain yes
	Hostname ssh.github.com
	IdentityFile ~/.ssh/personal
	TCPKeepAlive yes
	IdentitiesOnly yes
	# Port 443
	User git

Host github.flect
	AddKeysToAgent yes
	UseKeychain yes
	Hostname ssh.github.com
	IdentityFile ~/.ssh/flect
	TCPKeepAlive yes
	IdentitiesOnly yes
	Port 443
	User git
